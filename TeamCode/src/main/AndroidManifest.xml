        android:label="@string/app_name"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">

        <!-- Your main FTC RobotController activity (usually already present) -->
        <activity android:name="com.qualcomm.ftcrobotcontroller.FtcRobotControllerActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!-- Other FTC OpModes / activities go hereâ€¦ -->

        <!-- Preset Editor UI screen -->
        <activity
            android:name="org.firstinspires.ftc.teamcode.ui.PresetEditorActivity"
            android:exported="true" />

    </application>
</manifest>
